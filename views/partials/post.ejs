<div class="row mb-5">
  <% if (data.allPosts) data.allPosts.forEach( p => {%>

  <div class="col-md-6 col-lg-3">
    <a href="/post/<%= p.url %>"
      ><img class="img-fluid" src="<%=p.image.publicUrl%>"
    /></a>
  </div>
  <div class="col">
    <h3 class="name"><%= p.title %></h3>
    <p class="description">
      <%= p.description %>
    </p>
  </div>
  <%})%>
</div>
